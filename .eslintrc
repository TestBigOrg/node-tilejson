{
  "env": {
    "node": true
  },
  "rules": {
    "strict": [0],
    "curly": [0],
    "no-alert": [0],
    "no-native-reassign": [0],
    "new-cap": [0],
    "no-shadow": [0],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "camelcase": [0],
    "consistent-return": [0],
    "no-underscore-dangle": [0],
    "comma-spacing": [0],
    "key-spacing": [0],
    "no-use-before-define": [0]
  }
}
